{
  "extends": "../tsconfig.json",
  "name": "react-diagrams-demos",
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "../build/demos",
    "target": "es5",
    "declaration": false,
    "sourceMap": true,
    "skipLibCheck": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true,
    "jsx": "react",
    "lib": ["dom", "es2015"],
    "paths": {
      "@leiko/react-diagrams": ["../src/index"]
    }
  }
}
